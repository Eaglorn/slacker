                                                            prefWidth="914.0">
                                                    <columns>
                                                        <TableColumn2 fx:id="tableModelColumnId" prefWidth="100.0"
                                                                      text="Идентификатор"/>
                                                        <TableColumn2 fx:id="tableModelColumnName" prefWidth="270.0"
                                                                      text="Наименование"/>
                                                        <TableColumn2 fx:id="tableModelColumnMaker" prefWidth="270.0"
                                                                      text="Производитель"/>
                                                        <TableColumn2 fx:id="tableModelColumnTypeOfHardware"
                                                                      prefWidth="270.0" text="Тип оборудования"/>
                                                    </columns>
                                                </TableView2>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </Tab>
                                <Tab text="Адрес">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="117.0"
                                                    prefWidth="914.0"/>
                                    </content>
                                </Tab>
                                <Tab text="Руководитель">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="117.0"
                                                    prefWidth="914.0"/>
                                    </content>
                                </Tab>
                                <Tab text="Составитель">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="117.0"
                                                    prefWidth="914.0"/>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
